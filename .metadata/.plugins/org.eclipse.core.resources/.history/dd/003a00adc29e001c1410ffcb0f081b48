package variables;

public class basicmaths {
	static int p =10;
	static int q =20;
	
	void addition() 
	{
	int a = 3;
	int b = 4;
	System.out.println("a+b");
	}

	void division()
	{
		int a= 6;
		int b= 4;
		System.out.println("division" + (a/b));
	}
	
	void multipication()
	{
		int a= 4;
	    int b= 5;
	    System.out.println("multipication" +(a*b));
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
    basicmaths obj=new basicmaths();
    
    obj.addition();
    obj.division();
    obj.multipication();
    System.out.println("multipcation of p q" +(p*q));
	}
	
    void subtraction()
    {
    	int a= 4;
        int b=4;
    	System.out.println("substraction" +(a-b));
    }
}
