package collectionpratice;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.LinkedList;
import java.util.List;

public class arraypratice 
{
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
 
		
	//	ArrayList<String> studentname = new ArrayList<>();	
	//	studentname.add("tushar");
	//	List<Integer> list  = new ArrayList();
		ArrayList<Object> list = new ArrayList<Object>();
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(4);
		
		System.out.println(list);
		System.out.println(list.get(0));
	//	list.re
		list.add(1, 50);
		System.out.println(list);
		System.out.println(list.get(0));
		//list.clear();//it will clear all list
		//list.set(0, 63);
		System.out.println(list.contains(50));//result will be true and false
		
		for(int i=0; i < list.size();i++)
		{
			System.out.println("element is"+ list.get(i));
		}
		List<String> al=new LinkedList<String>();  
		  al.add("Ravi");  
		  al.add("Vijay");  
		  al.add("Ravi");  
		  al.add("Ajay");
		  al.get(0);
		  System.out.println(al.get(0));
		  al.isEmpty();
		  List<String> al1=new LinkedList<String>(); 
		  al1.add("java"); 
		  al1.add("python"); 
		  al1.add("ruby"); 
		  al.addAll(al1);
		  System.out.println(al);
		  al.addAll(3, al);
		  System.out.println(al);
		  
	}
}
