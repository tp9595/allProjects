package variables;

public class string {
	
	
	void stringliteral()
	{
		String s1 =  "test";
		String s2 =  "hello";
		
		System.out.print(s1);
		System.out.print(s2);
		
		 s1  = s1.concat(s2);
		
		System.out.println(s1);
		System.out.println(s2);
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		string obj = new string();
				obj.stringliteral();
	}

}
