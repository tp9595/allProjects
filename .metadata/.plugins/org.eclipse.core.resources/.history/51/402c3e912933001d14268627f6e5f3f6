package Stringprogramprac;

public class string1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
   String str = "grass is green";
   int count = 0;
    for(int i=0; i<str.length(); i++)
    {
    	if(str.charAt(i)!=' ')
    	{
    		count++;
    	}
    }
    System.out.println(count);
   
		
	}

}
