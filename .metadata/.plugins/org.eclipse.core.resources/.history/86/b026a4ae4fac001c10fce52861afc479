package variables;

public class forloop 
{
	
    void printnumber()
    {
	int n =3;
    	for(int i = 1; i<=3; i++ )
    	 {
    		for(int k = i ; k<=i; k++)
    		{
    			System.out.print(" ");
    		}
		 for(int k =i; k<=3; k++ ) 
		  { 
			  System.out.print("*"); 
		  }  
		 System.out.println();
    	}
    	
    	
		  
		 
    }
    
  public static void main(String[] args) {
	forloop obj = new forloop();
	obj.printnumber();
	
    }  
  
}
