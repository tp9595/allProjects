package variables;

public class oddandeven {
	
	int i = 1;
	
	void number()
	{
		while(i<=10)
		{
			if(i%2==0)
			{
				System.out.println("even number="+i);
			}
			i++;
				
	}
	
}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		oddandeven obj = new oddandeven();
	    obj.number();
		

	    }

}
